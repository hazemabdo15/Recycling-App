{
  "expo": {
    "name": "Karakeeb",
    "slug": "Karakeeb",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "karakeebapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "localization": {
      "supportsRtl": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.recyclecrew.karakeeb"
    },
    "android": {
      "package": "com.recyclecrew.karakeeb",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "usesCleartextTraffic": false,
      "networkSecurityConfig": {
        "domain-config": [
          {
            "domain": "api.groq.com",
            "cleartextTrafficPermitted": false
          }
        ]
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "recyclingapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "resizeMode": "cover",
          "backgroundColor": "#0E9F6E"
        }
      ],
      "expo-secure-store",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_GROQ_API_KEY": "you_groq_api_here",
      "router": {},
      "eas": {
        "projectId": "8271f86a-0640-46da-afcb-c71c36e4d3c6"
      }
    }
  }
}
